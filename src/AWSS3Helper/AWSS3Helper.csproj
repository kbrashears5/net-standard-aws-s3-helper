<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Kevin Brashears</Authors>
    <Company>kbrashears5</Company>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/kbrashears5/net-standard-aws-s3-helper</PackageProjectUrl>
    <RepositoryUrl>https://github.com/kbrashears5/net-standard-aws-s3-helper</RepositoryUrl>
    <Description>Collection of helper functions for interacting with the AWS S3 service</Description>
    <RepositoryType>github</RepositoryType>
    <PackageId>NetStandardAWSS3Helper</PackageId>
    <Product>NetStandardAWSS3Helper</Product>
    <PackageTags>aws;s3;helper;netStandard</PackageTags>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>documentation.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.S3" Version="3.7.0.2" />
    <PackageReference Include="NetStandardLogger" Version="2.0.6" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>
</Project>